{
	"name": "xxbun-cache",
	"version": "1.0.0",
	"description": "A Bun-native port of xxstache using bun:sqlite and xxhash64",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"files": [
		"dist/**/*",
		"README.md",
		"LICENSE"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"bun": "./dist/index.js"
		}
	},
	"engines": {
		"bun": ">=1.0.0"
	},
	"scripts": {
		"build": "bun build ./src/index.ts --outdir ./dist --target bun --sourcemap=external && bun run build:types",
		"build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
		"test": "bun test",
		"bench": "bun run benchmarks/bench.ts",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"clean": "rm -rf dist .tmp",
		"ci": "bun run lint && bun run test && bun run build",
		"prepublishOnly": "bun run build"
	},
	"keywords": [
		"cache",
		"disk-cache",
		"sqlite",
		"bun",
		"hybrid",
		"ttl",
		"lru",
		"xxhash",
		"xxbun-cache"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/YOUR_USERNAME/xxbun-cache.git"
	},
	"bugs": {
		"url": "https://github.com/YOUR_USERNAME/xxbun-cache/issues"
	},
	"homepage": "https://github.com/YOUR_USERNAME/xxbun-cache#readme",
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@types/bun": "latest",
		"@types/fs-extra": "^11.0.4"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"fs-extra": "^11.3.3",
		"xxhash-wasm": "^1.1.0"
	}
}